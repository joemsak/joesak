<%= form_tag(developer_sessions_path) do %>
  <div class='field text'>
    <%= label_tag :username %>
    <%= text_field_tag :username %>
  </div>

  <div class='field text password'>
    <%= label_tag :password %>
    <%= password_field_tag :password %>
  </div>

  <div class='button submit'>
    <%= submit_tag t('forms.buttons.sessions.sign_in') %>
  </div>
<% end %>
